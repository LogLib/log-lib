        "@clickhouse/client-web": "^0.2.0",
        "@hono/node-server": "^1.1.1",
        "@libsql/client": "^0.3.1",
        "@loglib/clickhouse": "1.0.0",
        "@loglib/db": "1.0.0",
        "@loglib/setup": "1.0.0",
        "@t3-oss/env-core": "^0.6.0",
        "@types/request-ip": "^0.0.38",
        "@types/tar": "^6.1.5",
        "@vercel/kv": "^0.2.1",
        "date-fns": "^2.30.0",
        "detect-browser": "^5.3.0",
        "dotenv": "^16.3.1",
        "drizzle-orm": "^0.28.5",
        "hono": "^3.3.4",
        "isbot": "^3.6.10",
        "jsonwebtoken": "^9.0.0",
        "kafkajs": "^2.2.4",
        "kysely": "^0.26.1",
        "kysely-planetscale": "^1.3.0",
        "maxmind": "^4.3.11",
        "ora": "^7.0.1",
